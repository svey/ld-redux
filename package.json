  "author": "Marcus Svehlak <marcus@svey.io> (http://svey.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/svey/ld-redux/issues"
  },
  "homepage": "https://github.com/svey/ld-redux#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.0",
    "babel-jest": "^23.4.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^5.5.0",
