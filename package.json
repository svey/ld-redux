  "dependencies": {
    "ip": "^1.1.5",
    "launchdarkly-js-client-sdk": "^3.1.3",
    "lodash.camelcase": "^4.3.0",
    "ua-parser-js": "^0.7.18",
    "uuid": "^3.3.2"
